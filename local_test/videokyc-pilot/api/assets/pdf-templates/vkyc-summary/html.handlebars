<html>

<head>
    <link rel="stylesheet" href="css/all.min.css">
    <link rel="stylesheet" href="css/template-styles.css">
    <link rel="stylesheet" href="css/test.css">
</head>

<body>
    <div class="main-wrapper main-wrapper-1">
        <div class="main-content" style="min-height: 348px;">
            <section class="section">
                <div class="section-body ">
                    <h4 class="text-center">Video KYC Report</h4>
                    <hr class="bg-primary border-2 border-top border-primary">

                    <div class="rows-container">
                        <div class="row">
                            <div class="col" style="width: 50%; ">
                                <div class="card">
                                    <div class="card-header">
                                        <h4>{{customerName}} / {{customerPAN}} </h4>
                                    </div>
                                    <div class="card-body">
                                        <table style="width: 100%;">
                                            <colgroup>
                                                <td style="width: 50%;"></td>
                                                <td style="width: 25%;"></td>
                                                <td style="width: 25%;"></td>
                                            </colgroup>

                                            <tr>
                                                <td style="vertical-align: center;">
                                                    <div class="profile-widget-header">
                                                        <img width="80%" height="80%"
                                                            class="rounded-circle profile-widget-picture "
                                                            src="data:image/jpeg;base64,">
                                                    </div>
                                                </td>

                                                <td style=" vertical-align: center;">
                                                    <div class="profile-widget-item">
                                                        <div class="profile-widget-item-label">KYC Status</div>
                                                        <div class="profile-widget-item-value text-primary">
                                                            {{ kycStatus}}
                                                        </div>
                                                    </div>
                                                </td>

                                                <td style=" vertical-align: center;">
                                                    <div class="profile-widget-item">
                                                        <div class="profile-widget-item-label">VKYC On</div>
                                                        <div class="profile-widget-item-value text-primary">
                                                            {{ kycDoneOn}}
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="col">
                                <div class="card card-primary">
                                    <div class="card-body ">
                                        <div class="col-md-6">
                                            <address><strong>Aadhar Address</strong><br> {{customerAadharAddress}}
                                            </address>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="rows-container">
                        <div class="row">

                            {{#if activityState.hasIpVerification}}
                            <div class="col" style="width: 50%;">
                                <div class="card card-primary">
                                    <div class="card-header">
                                        <h4>IP Address Information</h4>
                                        <div class="card-header-action"><i class="fa fa-check text-success "></i> </div>
                                    </div>
                                    <div class="card-body ">
                                        <div>
                                            <dl class="row">
                                                <dt class="col-sm-3">IP Address</dt>
                                                <dd class="col-sm-9">
                                                    {{activityData.ipVerification.ip}}</dd>
                                                <dt class="col-sm-3">Est. Lat/Lng</dt>
                                                <dd class="col-sm-9">
                                                    {{activityData.ipVerification.latitude}},
                                                    {{activityData.ipVerification.longitude}}</dd>
                                                <dt class="col-sm-3">Internet Provider</dt>
                                                <dd class="col-sm-9">
                                                    {{activityData.ipVerification.internetProvider}}
                                                </dd>
                                                <dt class="col-sm-3">City</dt>
                                                <dd class="col-sm-9">
                                                    {{activityData.ipVerification.city}}</dd>
                                                <dt class="col-sm-3">Country</dt>
                                                <dd class="col-sm-9">
                                                    {{activityData.ipVerification.country}}
                                                </dd>
                                                <dt class="col-sm-3">Threat Assessment</dt>
                                                <dd class="col-sm-9">
                                                    <div class="text-dark "> No Threats Found. The client's IP Address
                                                        seems
                                                        to be clean. </div>
                                                </dd>
                                            </dl>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{else}}
                            {{!-- do nothing --}}
                            {{/if}}

                            {{#if activityState.hasGeolocationVerification}}
                            <div class="col">
                                <div class="card card-primary">
                                    <div class="card-header">
                                        <h4>Geo Location Information</h4>
                                        <div class="card-header-action"><i class="fa fa-check text-success "></i> </div>
                                    </div>
                                    <div class="card-body ">
                                        <div class="col-md-6">
                                            <address><strong>Address</strong><br>
                                                {{activityData.geolocationVerification.formatted_address}}
                                            </address>
                                            <address><strong>Est. Lat/Lng</strong><br>
                                                {{activityData.geolocationVerification.lat}},
                                                {{activityData.geolocationVerification.lng}}
                                            </address>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{else}}
                            {{!-- do nothing --}}
                            {{/if}}

                        </div>
                    </div>


                    {{#if activityState.hasRandomQuestions}}
                    <div class="rows-container">
                        <div class="row">
                            <div class="col-12 ">
                                <div class="card card-primary">
                                    <div class="card-header">
                                        <h4>Random Questions</h4>
                                        <div class="card-header-action"><i class="fa fa-check text-success "></i> </div>
                                    </div>
                                    <div class="card-body ">

                                        {{#each activityData.randomQuestions}}
                                        <p class=""> Q{{index}} : <b>{{question}}</b><br><i
                                                class="fa fa-check text-success "></i> Ans : {{expectedAnswer}} </p>
                                        {{/each}}

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{else}}
                    {{!-- do nothing --}}
                    {{/if}}

                    <div class="rows-container">
                        <div class="row">

                            {{#if activityState.hasFaceRecognition}}
                            <div class="col" style="width: 50%;">
                                <div class="card card-primary">
                                    <div class="card-header">
                                        <h4>Face Recognition </h4>
                                        <div class="card-header-action">
                                            {{activityData.faceRecognition.matchingPercentage}} &nbsp; <i
                                                class="fa fa-check text-success "></i> </div>
                                    </div>
                                    <div class="card-body ">
                                        <div class="row">
                                            <div class="col" style="width: 41%;">
                                                <figure class="figure">
                                                    <img alt="Face Image" class="figure-img img-fluid rounded"
                                                        src="{{activityData.faceRecognition.aadharImageBase64}}">
                                                    <figcaption class="figure-caption">Aadhar Image </figcaption>
                                                </figure>

                                            </div>
                                            <div class="col">
                                                <figure class="figure">
                                                    <img alt="Face Image" class="figure-img img-fluid rounded"
                                                        src="{{activityData.faceRecognition.liveImageBase64}}">
                                                    <figcaption class="figure-caption">Live Image </figcaption>
                                                </figure>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{else}}
                            {{!-- do nothing --}}
                            {{/if}}


                            {{#if activityState.hasCustomerSignature}}

                            <div class="col">
                                <div class="card card-primary">
                                    <div class="card-header">
                                        <h4>Customer Signature</h4>
                                        <div class="card-header-action"><i class="fa fa-check text-success "></i> </div>
                                    </div>
                                    <div class="card-body">
                                        <figure class="figure"><img alt="Signature" class="figure-img img-fluid rounded"
                                                src="{{activityData.customerSignature.signatureBase64}}">
                                            <figcaption class="figure-caption">Signature</figcaption>
                                        </figure>
                                    </div>
                                </div>
                            </div>

                            {{else}}
                            {{!-- do nothing --}}
                            {{/if}}

                        </div>
                    </div>

                    {{#if activityState.hasPanCapture}}
                    <div class="rows-container">
                        <div class="row">
                            <div class="card card-primary">
                                <div class="card-header">
                                    <h4>PAN Information </h4>
                                </div>
                                <div class="card-body">
                                    <table style="width: 100%;">
                                        <colgroup>
                                            <td style="width: 50%;"></td>
                                            <td style="width: 50%;"></td>
                                        </colgroup>

                                        <tr>
                                            <td>
                                                <figure class="figure">
                                                    <img alt="Face Image" class="figure-img img-fluid rounded"
                                                        src="data:image/jpeg;base64,{{activityData.panCapture.panImageBase64}}">
                                                    <figcaption class="figure-caption">PAN Image </figcaption>
                                                </figure>
                                            </td>
                                            <td>
                                                <div>
                                                    <div class="alert alert-light">
                                                        {{activityData.panCapture.panNumber}} <br>
                                                        {{activityData.panCapture.name}}
                                                        <br>
                                                        S/O
                                                        {{activityData.panCapture.fatherName}} <br>
                                                        {{activityData.panCapture.dob}}
                                                    </div>
                                                </div>
                                                <div>
                                                    <figure class="figure"><img alt="Face Image"
                                                            class="figure-img img-fluid rounded"
                                                            src="data:image/jpeg;base64,{{activityData.panCapture.faceImageBase64}}">
                                                        <figcaption class="figure-caption">Face Image </figcaption>
                                                </div>
                                                <div>
                                                    <figure class="figure"><img alt="Face Image"
                                                            class="figure-img img-fluid rounded"
                                                            src="data:image/jpeg;base64,{{activityData.panCapture.signatureBase64}}">
                                                        <figcaption class="figure-caption">Signature Image </figcaption>
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{else}}
                    {{!-- do nothing --}}
                    {{/if}}

                    <div class="rows-container">
                        <div class="row">
                            <div class="col" style="width: 33%">
                                <h6>Powered by</h6> <img alt="Signature" class="figure-img img-fluid rounded"
                                    src="data:image/jpeg;base64, ">
                            </div>
                            <div class="col" style="width: 33%">
                                <i class="fas fa-envelope"></i> <a href="mailto:sales@botaiml.com">sales@botaiml.com</a>
                            </div>
                            <div class="col">
                                <i class="fas fa-phone"></i> +91-80-22250073/ 22257027/
                                22284587
                            </div>
                        </div>
                    </div>
                </div>
        </div>
        </section>
    </div>
    </div>
</body>

</html>